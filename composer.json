{
    "name": "justcommunication-ru/func-bundle",
    "description": "Func bundle",
    "type": "symfony-bundle",
    "keywords": ["functions","array","date","string"],
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "symfony/http-kernel": ">=5.4 ^6.2",
        "symfony/config": ">=5.4 ^6.2",
        "symfony/dependency-injection": ">=5.4 ^6.2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JustCommunication\\FuncBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JustCommunication\\FuncBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    }
}
